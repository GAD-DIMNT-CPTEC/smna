  229.  1 2                      dgeice = dgesmc(lay)
  230.  1 2                      if (dgeice .lt. 5.0_r8) stop 'ICE GENERALIZED EFFECTIVE SIZE OUT OF BOUNDS'
  231.  1 2                      if (dgeice .ge. 5.0_r8 .and. dgeice .le. 140._r8) then
  232.  1 2                         ncbands = 16
