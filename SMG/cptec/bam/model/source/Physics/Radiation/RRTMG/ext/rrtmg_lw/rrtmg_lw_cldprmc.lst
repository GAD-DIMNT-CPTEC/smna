  234.  1 2                         index = int(factor)
  235.  1 2                         if (index .eq. 46) index = 45
  236.  1 2                         fint = factor - float(index)
  237.  1 2                         ib = ngb(ig)
  238.  1 2                         abscoice(ig) = &
  239.  1 2                             absice3(index,ib) + fint * &
  240.  1 2                             (absice3(index+1,ib) - (absice3(index,ib)))
  241.  1 2                      elseif (dgeice .gt. 140._r8) then
  242.  1 2                         abscoice(ig) = absice0(1) + absice0(2)/radice
  243.  1 2                      endif
  244.  1 2       
  245.  1 2                   endif
  246.  1 2                      
  247.  1 2    ! Calculation of absorption coefficients due to water clouds.
  248.  1 2                   if (clwpmc(ig,lay) .eq. 0.0_r8) then
  249.  1 2                      abscoliq(ig) = 0.0_r8
  250.  1 2    
  251.  1 2                   elseif (liqflag .eq. 0) then
