  138.        contains
  139.        !------------------------------------------------------------------------------
  140.        subroutine get_solar_band_fraction_irrad(fractional_irradiance)
  141.           ! provide Solar Irradiance for each band in RRTMG
  142.        
  143.           ! fraction of solar irradiance in each band
  144.           real(r8), intent(out) :: fractional_irradiance(1:nswbands)
  145.           real(r8) :: tsi ! total solar irradiance
  146.        
  147.           tsi = sum(solar_ref_band_irradiance)
  148.  V--<>    fractional_irradiance = solar_ref_band_irradiance / tsi
  149.        
  150.        end subroutine get_solar_band_fraction_irrad
  151.        !------------------------------------------------------------------------------
  152.        subroutine get_ref_total_solar_irrad(tsi)
  153.           ! provide Total Solar Irradiance assumed by RRTMG
  154.        
  155.           real(r8), intent(out) :: tsi
  156.        
  157.           tsi = sum(solar_ref_band_irradiance)
  158.        
  159.        end subroutine get_ref_total_solar_irrad
  160.        !------------------------------------------------------------------------------
  161.        subroutine get_ref_solar_band_irrad( band_irrad )
  162.        
