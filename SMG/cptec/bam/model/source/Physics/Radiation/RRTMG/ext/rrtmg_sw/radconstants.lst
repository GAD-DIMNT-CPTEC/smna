  180.        subroutine get_lw_spectral_boundaries(low_boundaries, high_boundaries, units)
  181.           ! provide spectral boundaries of each longwave band
  182.        
  183.           real(r8), intent(out) :: low_boundaries(nlwbands), high_boundaries(nlwbands)
  184.           character(*), intent(in) :: units ! requested units
  185.        
  186.           select case (units)
  187.           case ('inv_cm','cm^-1','cm-1')
  188.  V--<>       low_boundaries  = wavenumber1_longwave
  189.  V--<>       high_boundaries = wavenumber2_longwave
  190.           case('m','meter','meters')
  191.  V--<>       low_boundaries  = 1.e-2_r8/wavenumber2_longwave
  192.  V--<>       high_boundaries = 1.e-2_r8/wavenumber1_longwave
  193.           case('nm','nanometer','nanometers')
  194.  V--<>       low_boundaries  = 1.e7_r8/wavenumber2_longwave
  195.  V--<>       high_boundaries = 1.e7_r8/wavenumber1_longwave
  196.           case('um','micrometer','micrometers','micron','microns')
  197.  V--<>       low_boundaries  = 1.e4_r8/wavenumber2_longwave
  198.  V--<>       high_boundaries = 1.e4_r8/wavenumber1_longwave
  199.           case('cm','centimeter','centimeters')
  200.  V--<>       low_boundaries  = 1._r8/wavenumber2_longwave
  201.  V--<>       high_boundaries = 1._r8/wavenumber1_longwave
  202.           case default
  203.              call endrun('get_lw_spectral_boundaries: spectral units not acceptable'//units)
  204.           end select
  205.        
  206.        end subroutine get_lw_spectral_boundaries
  207.        
