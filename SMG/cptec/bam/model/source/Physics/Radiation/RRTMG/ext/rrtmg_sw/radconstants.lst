  209.        subroutine get_sw_spectral_boundaries(low_boundaries, high_boundaries, units)
  210.           ! provide spectral boundaries of each shortwave band
  211.        
  212.           real(r8), intent(out) :: low_boundaries(nswbands), high_boundaries(nswbands)
  213.           character(*), intent(in) :: units ! requested units
  214.        
  215.           select case (units)
  216.           case ('inv_cm','cm^-1','cm-1')
  217.  V--<>       low_boundaries = wavenum_low
  218.  V--<>       high_boundaries = wavenum_high
  219.           case('m','meter','meters')
  220.  V--<>       low_boundaries = 1.e-2_r8/wavenum_high
  221.  V--<>       high_boundaries = 1.e-2_r8/wavenum_low
  222.           case('nm','nanometer','nanometers')
  223.  V--<>       low_boundaries = 1.e7_r8/wavenum_high
  224.  V--<>       high_boundaries = 1.e7_r8/wavenum_low
  225.           case('um','micrometer','micrometers','micron','microns')
  226.  V--<>       low_boundaries = 1.e4_r8/wavenum_high
  227.  V--<>       high_boundaries = 1.e4_r8/wavenum_low
  228.           case('cm','centimeter','centimeters')
  229.  V--<>       low_boundaries  = 1._r8/wavenum_high
  230.  V--<>       high_boundaries = 1._r8/wavenum_low
  231.           case default
  232.              call endrun('rad_constants.F90: spectral units not acceptable'//units)
  233.           end select
  234.        
  235.        end subroutine get_sw_spectral_boundaries
