  142.  1--<       do jk=1, nlayers
  143.  1             if (.not.lrtchk(jk)) then
  144.  1                pref(jk) =0._r8
  145.  1                ptra(jk) =1._r8
  146.  1                prefd(jk)=0._r8
  147.  1                ptrad(jk)=1._r8
  148.  1             else
  149.  1                zto1=ptau(jk)
  150.  1                zw  =pw(jk)
  151.  1                zg  =pgg(jk)  
  152.  1    
  153.  1    ! General two-stream expressions
  154.  1    
  155.  1                zg3= 3._r8 * zg
  156.  1                if (kmodts == 1) then
  157.  1                   zgamma1= (7._r8 - zw * (4._r8 + zg3)) * 0.25_r8
  158.  1                   zgamma2=-(1._r8 - zw * (4._r8 - zg3)) * 0.25_r8
  159.  1                   zgamma3= (2._r8 - zg3 * prmuz ) * 0.25_r8
  160.  1                else if (kmodts == 2) then  
  161.  1                   zgamma1= (8._r8 - zw * (5._r8 + zg3)) * 0.25_r8
  162.  1                   zgamma2=  3._r8 *(zw * (1._r8 - zg )) * 0.25_r8
  163.  1                   zgamma3= (2._r8 - zg3 * prmuz ) * 0.25_r8
  164.  1                else if (kmodts == 3) then  
  165.  1                   zgamma1= zsr3 * (2._r8 - zw * (1._r8 + zg)) * 0.5_r8
  166.  1                   zgamma2= zsr3 * zw * (1._r8 - zg ) * 0.5_r8
  167.  1                   zgamma3= (1._r8 - zsr3 * zg * prmuz ) * 0.5_r8
  168.  1                end if
  169.  1                zgamma4= 1._r8 - zgamma3
  170.  1        
  171.  1    ! Recompute original s.s.a. to test for conservative solution
  172.  1    
  173.  1                zwo= zw / (1._r8 - (1._r8 - zw) * (zg / (1._r8 - zg))**2)
  174.  1        
  175.  1                if (zwo >= zwcrit) then
  176.  1    ! Conservative scattering
  177.  1    
  178.  1                   za  = zgamma1 * prmuz 
  179.  1                   za1 = za - zgamma3
  180.  1                   zgt = zgamma1 * zto1
  181.  1            
  182.  1    ! Homogeneous reflectance and transmittance,
  183.  1    ! collimated beam
  184.  1    
  185.  1                   ze1 = min ( zto1 / prmuz , 500._r8)
  186.  1    !               ze2 = exp( -ze1 )
  187.  1    
  188.  1    ! Use exponential lookup table for transmittance, or expansion of 
  189.  1    ! exponential for low tau
  190.  1                   if (ze1 .le. od_lo) then 
  191.  1                      ze2 = 1._r8 - ze1 + 0.5_r8 * ze1 * ze1
  192.  1                   else
  193.  1                      tblind = ze1 / (bpade + ze1)
  194.  1                      itind = tblint * tblind + 0.5_r8
  195.  1                      ze2 = exp_tbl(itind)
  196.  1                   endif
  197.  1    !
  198.  1    
  199.  1                   pref(jk) = (zgt - za1 * (1._r8 - ze2)) / (1._r8 + zgt)
  200.  1                   ptra(jk) = 1._r8 - pref(jk)
  201.  1    
  202.  1    ! isotropic incidence
  203.  1    
  204.  1                   prefd(jk) = zgt / (1._r8 + zgt)
  205.  1                   ptrad(jk) = 1._r8 - prefd(jk)        
  206.  1    
  207.  1    ! This is applied for consistency between total (delta-scaled) and direct (unscaled) 
  208.  1    ! calculations at very low optical depths (tau < 1.e-4) when the exponential lookup
  209.  1    ! table returns a transmittance of 1.0.
  210.  1                   if (ze2 .eq. 1.0_r8) then 
  211.  1                      pref(jk) = 0.0_r8
  212.  1                      ptra(jk) = 1.0_r8
  213.  1                      prefd(jk) = 0.0_r8
  214.  1                      ptrad(jk) = 1.0_r8
  215.  1                   endif
  216.  1    
  217.  1                else
  218.  1    ! Non-conservative scattering
  219.  1    
  220.  1                   za1 = zgamma1 * zgamma4 + zgamma2 * zgamma3
  221.  1                   za2 = zgamma1 * zgamma3 + zgamma2 * zgamma4
  222.  1                   zrk = sqrt ( zgamma1**2 - zgamma2**2)
  223.  1                   zrp = zrk * prmuz               
  224.  1                   zrp1 = 1._r8 + zrp
  225.  1                   zrm1 = 1._r8 - zrp
  226.  1                   zrk2 = 2._r8 * zrk
  227.  1                   zrpp = 1._r8 - zrp*zrp
  228.  1                   zrkg = zrk + zgamma1
  229.  1                   zr1  = zrm1 * (za2 + zrk * zgamma3)
  230.  1                   zr2  = zrp1 * (za2 - zrk * zgamma3)
  231.  1                   zr3  = zrk2 * (zgamma3 - za2 * prmuz )
  232.  1                   zr4  = zrpp * zrkg
  233.  1                   zr5  = zrpp * (zrk - zgamma1)
  234.  1                   zt1  = zrp1 * (za1 + zrk * zgamma4)
  235.  1                   zt2  = zrm1 * (za1 - zrk * zgamma4)
  236.  1                   zt3  = zrk2 * (zgamma4 + za1 * prmuz )
  237.  1                   zt4  = zr4
  238.  1                   zt5  = zr5
  239.  1                   zbeta = (zgamma1 - zrk) / zrkg !- zr5 / zr4
  240.  1            
  241.  1    ! Homogeneous reflectance and transmittance
  242.  1    
  243.  1                   ze1 = min ( zrk * zto1, 500._r8)
  244.  1                   ze2 = min ( zto1 / prmuz , 500._r8)
  245.  1    !
  246.  1    ! Original
  247.  1    !              zep1 = exp( ze1 )
  248.  1    !              zem1 = exp(-ze1 )
  249.  1    !              zep2 = exp( ze2 )
  250.  1    !              zem2 = exp(-ze2 )
  251.  1    !
  252.  1    ! Revised original, to reduce exponentials
  253.  1    !              zep1 = exp( ze1 )
  254.  1    !              zem1 = 1._r8 / zep1
  255.  1    !              zep2 = exp( ze2 )
  256.  1    !              zem2 = 1._r8 / zep2
  257.  1    !
  258.  1    ! Use exponential lookup table for transmittance, or expansion of 
  259.  1    ! exponential for low tau
  260.  1                   if (ze1 .le. od_lo) then 
  261.  1                      zem1 = 1._r8 - ze1 + 0.5_r8 * ze1 * ze1
  262.  1                      zep1 = 1._r8 / zem1
  263.  1                   else
  264.  1                      tblind = ze1 / (bpade + ze1)
  265.  1                      itind = tblint * tblind + 0.5_r8
  266.  1                      zem1 = exp_tbl(itind)
  267.  1                      zep1 = 1._r8 / zem1
  268.  1                   endif
  269.  1    
  270.  1                   if (ze2 .le. od_lo) then 
  271.  1                      zem2 = 1._r8 - ze2 + 0.5_r8 * ze2 * ze2
  272.  1                      zep2 = 1._r8 / zem2
  273.  1                   else
  274.  1                      tblind = ze2 / (bpade + ze2)
  275.  1                      itind = tblint * tblind + 0.5_r8
  276.  1                      zem2 = exp_tbl(itind)
  277.  1                      zep2 = 1._r8 / zem2
  278.  1                   endif
  279.  1    
  280.  1    ! collimated beam
  281.  1    
  282.  1                   zdenr = zr4*zep1 + zr5*zem1
  283.  1                   zdent = zt4*zep1 + zt5*zem1
  284.  1                   if (zdenr .ge. -eps .and. zdenr .le. eps) then
  285.  1                      pref(jk) = eps
  286.  1                      ptra(jk) = zem2
  287.  1                   else
  288.  1                      pref(jk) = zw * (zr1*zep1 - zr2*zem1 - zr3*zem2) / zdenr
  289.  1                      ptra(jk) = zem2 - zem2 * zw * (zt1*zep1 - zt2*zem1 - zt3*zep2) / zdent
  290.  1                   endif 
  291.  1    
  292.  1    ! diffuse beam
  293.  1    
  294.  1                   zemm = zem1*zem1
  295.  1                   zdend = 1._r8 / ( (1._r8 - zbeta*zemm ) * zrkg)
  296.  1                   prefd(jk) =  zgamma2 * (1._r8 - zemm) * zdend
  297.  1                   ptrad(jk) =  zrk2*zem1*zdend
  298.  1    
  299.  1                endif
  300.  1    
  301.  1             endif         
