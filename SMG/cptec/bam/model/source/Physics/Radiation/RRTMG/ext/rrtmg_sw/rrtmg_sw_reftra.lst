  119.       
  120.             real(kind=r8) :: tblind
  121.             real(kind=r8) :: za, za1, za2
  122.             real(kind=r8) :: zbeta, zdend, zdenr, zdent
  123.             real(kind=r8) :: ze1, ze2, zem1, zem2, zemm, zep1, zep2
