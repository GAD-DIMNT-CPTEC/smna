  103.       
  104.       ! ------- Output -------
  105.       
  106.             real(kind=r8), intent(inout) :: pref(1:nlayers+1)                    ! direct beam reflectivity
  107.                                                                        !   Dimensions: (nlayers+1)
  108.             real(kind=r8), intent(inout) :: prefd(1:nlayers+1)                ! diffuse beam reflectivity
  109.                                                                        !   Dimensions: (nlayers+1)
  110.             real(kind=r8), intent(inout) :: ptra(1:nlayers+1)                    ! direct beam transmissivity
  111.                                                                        !   Dimensions: (nlayers+1)
  112.             real(kind=r8), intent(inout) :: ptrad(1:nlayers+1)                  ! diffuse beam transmissivity
  113.                                                                        !   Dimensions: (nlayers+1)
  114.       
