  171.          function initialize_scalar(seed) result(twister)
  172.            integer,       intent(in   ) :: seed
  173.            type(randomNumberSequence)                :: twister 
  174.            
  175.            integer :: i
  176.            ! See Knuth TAOCP Vol2. 3rd Ed. P.106 for multiplier. In the previous versions, 
  177.            !   MSBs of the seed affect only MSBs of the array state[].                       
  178.            !   2002/01/09 modified by Makoto Matsumoto            
  179.            
  180.            twister%state(0) = iand(seed, -1)
  181.  1---<     do i = 1,  blockSize - 1 ! ubound(twister%state)
  182.  1            twister%state(i) = 1812433253 * ieor(twister%state(i-1), &
  183.  1                                                 ishft(twister%state(i-1), -30)) + i
  184.  1            twister%state(i) = iand(twister%state(i), -1) ! for >32 bit machines
  185.  1--->     end do
  186.            twister%currentElement = blockSize
  187.          end function initialize_scalar
  188.          ! -------------------------------------------------------------
  189.          function initialize_vector(seed) result(twister)
  190.            integer, dimension(0:), intent(in) :: seed
  191.            type(randomNumberSequence)                      :: twister 
  192.            
  193.            integer :: i, j, k, nFirstLoop, nWraps
  194.            
  195.            nWraps  = 0
  196.            twister = initialize_scalar(19650218)
  197.            
  198.            nFirstLoop = max(blockSize, size(seed))
  199.  1---<     do k = 1, nFirstLoop
  200.  1            i = mod(k + nWraps, blockSize)
  201.  1            j = mod(k - 1,      size(seed))
  202.  1            if(i == 0) then
  203.  1              twister%state(i) = twister%state(blockSize - 1)
  204.  1              twister%state(1) = ieor(twister%state(1),                                 &
  205.  1                                      ieor(twister%state(1-1),                          & 
  206.  1                                           ishft(twister%state(1-1), -30)) * 1664525) + & 
  207.  1                                 seed(j) + j ! Non-linear
  208.  1              twister%state(i) = iand(twister%state(i), -1) ! for >32 bit machines
  209.  1              nWraps = nWraps + 1
  210.  1            else
  211.  1              twister%state(i) = ieor(twister%state(i),                                 &
  212.  1                                      ieor(twister%state(i-1),                          & 
  213.  1                                           ishft(twister%state(i-1), -30)) * 1664525) + & 
  214.  1                                 seed(j) + j ! Non-linear
  215.  1              twister%state(i) = iand(twister%state(i), -1) ! for >32 bit machines
  216.  1           end if
  217.  1--->     end do
  218.            
  219.            !
  220.            ! Walk through the state array, beginning where we left off in the block above
  221.            ! 
  222.  1---<     do i = mod(nFirstLoop, blockSize) + nWraps + 1, blockSize - 1
  223.  1           twister%state(i) = ieor(twister%state(i),                                 &
  224.  1                                   ieor(twister%state(i-1),                          & 
  225.  1                                        ishft(twister%state(i-1), -30)) * 1566083941) - i ! Non-linear
  226.  1           twister%state(i) = iand(twister%state(i), -1) ! for >32 bit machines
  227.  1--->     end do
  228.            
  229.            twister%state(0) = twister%state(blockSize - 1) 
  230.            
  231.  1---<     do i = 1, mod(nFirstLoop, blockSize) + nWraps
  232.  1           twister%state(i) = ieor(twister%state(i),                                 &
  233.  1                                   ieor(twister%state(i-1),                          & 
  234.  1                                        ishft(twister%state(i-1), -30)) * 1566083941) - i ! Non-linear
  235.  1           twister%state(i) = iand(twister%state(i), -1) ! for >32 bit machines
  236.  1--->     end do
  237.            
  238.            twister%state(0) = UMASK 
  239.            twister%currentElement = blockSize
  240.            
  241.          end function initialize_vector
  242.          ! -------------------------------------------------------------
  243.          ! Public functions
  244.          ! --------------------
  245.          function getRandomInt(twister)
  246.            type(randomNumberSequence), intent(inout) :: twister
  247.            integer                        :: getRandomInt
  248.            ! Generate a random integer on the interval [0,0xffffffff]
  249.            !   Equivalent to genrand_int32 in the C code. 
  250.            !   Fortran doesn't have a type that's unsigned like C does, 
  251.            !   so this is integers in the range -2**31 - 2**31
  252.            ! All functions for getting random numbers call this one, 
  253.            !   then manipulate the result
  254.            
  255.            if(twister%currentElement >= blockSize) call nextState(twister)
  256.              
  257.            getRandomInt = temper(twister%state(twister%currentElement))
  258.            twister%currentElement = twister%currentElement + 1
  259.          
  260.          end function getRandomInt
  261.          ! --------------------
  262.          function getRandomPositiveInt(twister)
  263.            type(randomNumberSequence), intent(inout) :: twister
  264.            integer                        :: getRandomPositiveInt
  265.            ! Generate a random integer on the interval [0,0x7fffffff]
  266.            !   or [0,2**31]
  267.            !   Equivalent to genrand_int31 in the C code. 
  268.            
  269.            ! Local integers
  270.            integer :: localInt
  271.        
  272.            localInt = getRandomInt(twister)
  273.            getRandomPositiveInt = ishft(localInt, -1)
  274.          
  275.          end function getRandomPositiveInt
  276.          ! --------------------
  277.        !! mji - modified Jan 2007, double converted to rrtmg real kind type
  278.          function getRandomReal(twister)
  279.            type(randomNumberSequence), intent(inout) :: twister
  280.        !    double precision             :: getRandomReal
  281.            real(kind=r8)             :: getRandomReal
  282.            ! Generate a random number on [0,1]
  283.            !   Equivalent to genrand_real1 in the C code
  284.            !   The result is stored as double precision but has 32 bit resolution
  285.            
  286.            integer :: localInt
  287.            
  288.            localInt = getRandomInt(twister)
  289.            if(localInt < 0) then
  290.        !      getRandomReal = dble(localInt + 2.0d0**32)/(2.0d0**32 - 1.0d0)
  291.              getRandomReal = (localInt + 2.0**32_r8)/(2.0**32_r8 - 1.0_r8)
  292.            else
  293.        !      getRandomReal = dble(localInt            )/(2.0d0**32 - 1.0d0)
  294.              getRandomReal = (localInt            )/(2.0**32_r8 - 1.0_r8)
  295.            end if
  296.        
  297.          end function getRandomReal
